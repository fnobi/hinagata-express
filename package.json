{
  "name": "hinagata-express",
  "description": "hinagata-express service server.",
  "version": "0.1.0",
  "scripts": {
    "test": "NODE_ENV=test mocha --reporter spec",
    "prestart": "npm install",
    "init-db": "sequelize init --config sequelize/config.json --migrations-path sequelize/migrations --seeders-path sequelize/seeders --url postgres://localhost/hinagata_express_development --force",
    "start": "NODE_ENV=production forever start --sourceDir . --pidFile hinagata-express.example.com.pid -l hinagata-express.example.com.log --append server.js"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "async": "0.2.*",
    "chai": "1.6.*",
    "config": "0.4.*",
    "express": "3.3.*",
    "jade": "1.5.*",
    "pg": "4.4.*",
    "pg-hstore": "2.3.*",
    "request": "2.48.*",
    "sequelize": "3.15.*",
    "sequelize-cli": "^2.3.1"
  },
  "devDependencies": {
    "mocha": "2.3.*"
  }
}
